{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Current File with Enlighten",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {"PROGRESS_BACKEND": "enlighten"}
        }
    ]
}
